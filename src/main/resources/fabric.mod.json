{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "ES Addon",
  "description": "An addon template for Meteor Client addons.",
  "authors": [
    "Ebatte_Sratte"
  ],
  "contact": {
    "sources": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.es.addon.EsAddon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.9"],
    "meteor-client": "*"
  }
}
